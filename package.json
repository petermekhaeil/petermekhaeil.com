{
  "name": "petermekhaeil.com",
  "version": "1.0.0",
  "description": "Peter Mekhaeil's Site",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production npm run build:css && eleventy",
    "build:css": "tailwindcss -i ./src/styles.css -o ./src/assets/output.css",
    "clean": "rm -rf ./_site",
    "dev": "concurrently \"npm run dev:css\" \"eleventy --serve\"",
    "dev:css": "tailwindcss -i ./src/styles.css -o ./src/assets/output.css --watch",
    "format": "prettier --write ./src/*.njk --parser html",
    "postinstall": "cd ./eleventy-plugin-embed-tweet && npm install"
  },
  "keywords": [],
  "author": "Peter Mekhaeil <mekhaeil.peter@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "1.0.0",
    "@11ty/eleventy-navigation": "0.3.1",
    "@11ty/eleventy-plugin-inclusive-language": "1.0.3",
    "@11ty/eleventy-plugin-rss": "1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "3.1.1",
    "@11tyrocks/eleventy-plugin-emoji-readtime": "1.0.1",
    "@sardine/eleventy-plugin-external-links": "1.3.2",
    "concurrently": "^6.4.0",
    "eleventy-plugin-images-responsiver": "^1.8.2",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "tailwindcss": "^3.0.0"
  },
  "dependencies": {
    "@11ty/eleventy-img": "^0.3.0",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.0.2",
    "eleventy-plugin-tailwindcss": "0.3.0",
    "eleventy-plugin-toc": "^1.1.0",
    "luxon": "^1.25.0",
    "markdown-it": "^11.0.0",
    "markdown-it-anchor": "^6.0.1",
    "markdown-it-attrs": "^3.0.3",
    "prism-theme-night-owl": "^1.4.0"
  }
}
